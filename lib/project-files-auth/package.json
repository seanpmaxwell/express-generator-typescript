{
  "name": "express-typescript-example",
  "version": "0.0.0",
  "scripts": {
    "build": "npx ts-node ./scripts/build.ts",
    "dev": "NODE_ENV=development npx ts-node ./src",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src -e ts",
    "lint": "npx eslint ./src",
    "lint:tests": "npx eslint ./spec",
    "start": "NODE_ENV=production node -r ./preload.js ./dist",
    "test": "NODE_ENV=test npx ts-node ./spec",
    "test:hot": "nodemon --exec \"npm run test\" --watch ./src --watch ./spec -e ts"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.0",
    "inserturlparams": "^1.0.1",
    "jet-logger": "^1.3.1",
    "jet-paths": "^1.0.5",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@eslint/js": "^9.11.1",
    "@stylistic/eslint-plugin-ts": "^2.8.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "^4.3.0",
    "@types/jsonfile": "^6.1.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.14",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.11.1",
    "eslint-plugin-n": "^17.10.3",
    "find": "^0.3.0",
    "fs-extra": "^10.1.0",
    "jasmine": "^4.3.0",
    "nodemon": "^2.0.19",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  }
}
